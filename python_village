def c_square(file):
    a,b = map(int, open(file, 'rt').read().split())
    return a**2 + b**2


def splice(filename):
    with open(filename, 'r') as file:
        s = file.readline(200)
        a, b, c, d = map(int, file.readline().split())
    print s[a:b+1] + ' ' + s[c:d+1]

def sum_odd(file):
  a, b = map(int, open(file, 'rt').read().split())
  if not a < b < 10000:
    return "Invalid input"
  
  sum = 0
  for i in range(a,b+1):
    if i%2 != 0:
      sum = sum + i
  return sum
